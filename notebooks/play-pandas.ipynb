    "import pandas as pd\n",
    "\n",
    "# 例のデータフレーム\n",
    "df = pd.DataFrame({\n",
    "    'Name': ['Alice', 'Bob', 'Charlie'],\n",
    "    'Gender': ['Female', 'Male', 'Male']\n",
    "})\n",
    "\n",
    "# Gender列をダミー変数に変換\n",
    "df_dummies = pd.get_dummies(df, columns=['Gender'])\n",
    "\n",
    "df_dummies"
   ]
